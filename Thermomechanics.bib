	pages = {61--112},
	file = {simo_1992_algorithms_for_static_and_dynamic_multiplicative_plasticity_that_preserve_the.pdf:/home/zeluis/Zotero/storage/S2MTAR54/simo_1992_algorithms_for_static_and_dynamic_multiplicative_plasticity_that_preserve_the.pdf:application/pdf},
}

