@article{michler_relevance_2003,
	title = {The relevance of conservation for stability and accuracy of numerical methods for fluid{\textendash}structure interaction},
	volume = {192},
	issn = {0045-7825},
	url = {https://www.sciencedirect.com/science/article/pii/S004578250300392X},
	doi = {https://doi.org/10.1016/S0045-7825(03)00392-X},
	abstract = {Numerical simulation of fluid{\textendash}structure interactions has typically been done using partitioned solution methods. However, partitioned methods are inherently non-conservative and generally numerically unstable. The deficiencies of partitioned methods have motivated the investigation of monolithic solution methods. Conservation is possible for monolithic methods, the conditions have recently been presented [E.H. van Brummelen, S.J. Hulshoff, R. de Borst, Energy conservation under incompatibility for fluid{\textendash}structure interaction problems, Comput. Methods Appl. Mech. Engrg. 192 (2003) 2727{\textendash}2748]. In the present paper we investigate the relevance of maintaining conservation for a model fluid{\textendash}structure interaction problem, viz., the piston problem. To distinguish the effect of the error induced by the interface coupling from the fluid and structure discretization errors, we use fluid subcycling and an exact time-integration method for the structure. A comparison between conservative and non-conservative monolithic methods as well as partitioned methods is made. We show that maintaining conservation has considerable impact on the stability and accuracy of the numerical method. These results also indicate that only for a conservative monolithic scheme the improvement in accuracy over partitioned methods warrants the computational cost associated with a monolithic solution. Moreover, we illustrate the implications that particular combinations of fluid and structure discretizations can have on the conservation properties of the fluid{\textendash}structure interaction problem.},
	number = {37},
	journal = {Computer Methods in Applied Mechanics and Engineering},
	author = {Michler, C. and Brummelen, E. H. van and Hulshoff, S. J. and Borst, R. de},
	year = {2003},
	keywords = {Fluid{\textendash}structure interaction, Accuracy, Energy conservation, Monolithic schemes, Numerical stability, Partitioned schemes, Space{\textendash}time formulation, Time-discontinuous Galerkin method, to read, unclassified},
	pages = {4195--4215},
	file = {Michler et al. - 2003 - The relevance of conservation for stability and ac.pdf:/home/zeluis/Zotero/storage/J5Q7KSWH/michler2003.pdf:application/pdf;Michler et al. - 2003 - The relevance of conservation for stability and ac.pdf:/home/zeluis/Zotero/storage/44L3YB3K/Michler et al. - 2003 - The relevance of conservation for stability and ac.pdf:application/pdf},
