	keywords = {to read, unclassified},
	pages = {1091--1110},
	file = {seitz_et_al_2019_nitsche{\textquoteright}s_method_for_finite_deformation_thermomechanical_contact_problems.pdf:/home/zeluis/Zotero/storage/X9PUQZCT/seitz_et_al_2019_nitsche{\textquoteright}s_method_for_finite_deformation_thermomechanical_contact_problems.pdf:application/pdf},
