	author = {Rieger, A. and Wriggers, P.},
	year = {2004},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/nme.900},
	keywords = {adaptive methods, contact mechanics, finite element methods, thermo-mechanics, classified, read},
	pages = {871--894},
	file = {Snapshot:/home/zeluis/Zotero/storage/NKGCBCW3/nme.html:text/html;rieger_wriggers_2004_adaptive_methods_for_thermomechanical_coupled_contact_problems.pdf:/home/zeluis/Zotero/storage/XGIZ3DML/rieger_wriggers_2004_adaptive_methods_for_thermomechanical_coupled_contact_problems.pdf:application/pdf},
