	keywords = {Finite elements, Fluid{\textendash}structure interaction, Mortar method, Dual Lagrange multipliers, Non-conforming meshes},
	pages = {3111--3126},
	file = {ScienceDirect Snapshot:/home/zeluis/Zotero/storage/3LM4DI9Q/S0045782511002246.html:text/html;kl{\"o}ppel_et_al_2011_fluid{\textendash}structure_interaction_for_non-conforming_interfaces_based_on_a_dual.pdf:/home/zeluis/Zotero/storage/UJDAEMAW/kl{\"o}ppel_et_al_2011_fluid{\textendash}structure_interaction_for_non-conforming_interfaces_based_on_a_dual.pdf:application/pdf},
