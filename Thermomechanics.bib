	author = {Matthies, Hermann G. and Niekamp, Rainer and Steindorf, Jan},
	month = mar,
	year = {2006},
	keywords = {Block-Newton methods, Fluid{\textendash}structure interaction, Partitioned methods, Quasi-Newton methods, Strong coupling, classified, to read},
	pages = {2028--2049},
	file = {matthies_et_al_2006_algorithms_for_strong_coupling_procedures.pdf:/home/zeluis/Zotero/storage/IYM8AUZA/matthies2006.pdf:application/pdf;Matthies et al. - 2006 - Algorithms for strong coupling procedures.pdf:/home/zeluis/Zotero/storage/ZTYHJFVS/Matthies et al. - 2006 - Algorithms for strong coupling procedures.pdf:application/pdf},
